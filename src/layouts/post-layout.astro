---
import Layout from '@/layouts/base-layout.astro'

interface Props {
  category: string
  publishedAt: Date
  title: string
}

const { category, title } = Astro.props
---

<Layout label={category} title={title}>
  <article
    class="prose max-w-none dark:prose-invert prose-p:text-atom-dark prose-a:text-primary dark:prose-p:text-atom dark:prose-a:text-primary-dark"
  >
    <slot />
  </article>
</Layout>
