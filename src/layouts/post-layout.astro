---
import Layout from '@/layouts/base-layout.astro'

interface Props {
  category: string
  publishedAt: Date
  title: string
}

const { category, title } = Astro.props
---

<Layout label={category} title={title}>
  <article
    class="xat-typo max-w-none prose prose-truegray dark:prose-invert"
  >
    <slot />
  </article>
</Layout>

